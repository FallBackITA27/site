ody>\n    </table>\n  </div>\n</template>\n    \n    \n<script>\nimport { useRoute } from 'vue-router';\n  \nexport default {\n  name: 'TrackDetailsView',\n  data() {\n    return {\n        times: null,\n    }\n  },\n  methods: {\n      getTrackData : async function () {\n          const route = useRoute()\n          this.times = await import(`../data/times/${ route.params.extension }/${ route.params.category }/${ route.params.format }/${ route.params.track }.json`).then(module => {\n              return module.default;\n          });\n      }\n  },\n  async created(){\n      await this.getTrackData();\n  }\n}\n</script>\n\n<style scoped>\ntable, th, tr, td {\n  border: 1px solid white;\n  border-collapse: collapse;\n}\n\n#ranking-table-details {\n  width: 100%;\n  text-align: center;\n}\n</style>"],"mappings":"AA+BA,SAASA,QAAO,QAAS,YAAY;AAErC,eAAe;EACbC,IAAI,EAAE,kBAAkB;EACxBC,IAAIA,CAAA,EAAG;IACL,OAAO;MACHC,KAAK,EAAE;IACX;EACF,CAAC;EACDC,OAAO,EAAE;IACLC,YAAW,EAAI,eAAAA,CAAA,EAAkB;MAC7B,MAAMC,KAAI,GAAIN,QAAQ,EAAC;MACvB,IAAI,CAACG,KAAI,GAAI,MAAM,MAAM,CAAE,iBAAiBG,KAAK,CAACC,MAAM,CAACC,SAAW,IAAIF,KAAK,CAACC,MAAM,CAACE,QAAU,IAAIH,KAAK,CAACC,MAAM,CAACG,MAAQ,IAAIJ,KAAK,CAACC,MAAM,CAACI,KAAO,OAAM,CAAC,CAACC,IAAI,CAACC,MAAK,IAAK;QACnK,OAAOA,MAAM,CAACC,OAAO;MACzB,CAAC,CAAC;IACN;EACJ,CAAC;EACD,MAAMC,OAAOA,CAAA,EAAE;IACX,MAAM,IAAI,CAACV,YAAY,EAAE;EAC7B;AACF"},"metadata":{},"sourceType":"module","externalDependencies":[]}